generator client {
  provider = "prisma-client"
  output = "../src/generated/prisma
  moduleFormat = "cjs"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        Int       @id @default(autoincrement())
  email     String    @unique
  username  String    @unique
  createdAt Datetime  @default(now())
  updatedAt Datetime  @updatedAt
}
